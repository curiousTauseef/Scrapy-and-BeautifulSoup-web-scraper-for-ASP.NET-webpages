    <script type="text/javascript">
        $(function () {
            ShowProgress();
        });
        function ShowProgress() {
            setTimeout(function () {
                var modal = $('<div />');
                modal.addClass("modal");
                $('#mapConatiner').append(modal);
                var loading = $(".loading");
                loading.show();
                var position = $('mapConatiner').position();

                var top = Math.max($('.mapConatiner').height() / 2 - loading[0].offsetHeight / 2, 0);
                var left = Math.max($('mapConatiner').width() / 2 - loading[0].offsetWidth / 2, 0);
                loading.css({ top: top, left: left });
            }, 200);
        }
        function getTop(e) {
            var offset = e.offsetTop;
            if (e.offsetParent != null) offset += getTop(e.offsetParent);
            return offset;
        }
        function getLeft(e) {
            var offset = e.offsetLeft;
            if (e.offsetParent != null) offset += getLeft(e.offsetParent);
            return offset;
        }
        function hideDivImageDisplay() {
            $('#divImageDisplay').html("");
        }

        function showDivImageDisplay(img) {
            var grid = $('#ContentPlaceHolder1_gvApplicationList');
            var leftPos = getLeft(img) - 280;
            var topPos = getTop(img) - 170;
            $('#divImageDisplay').offset({ top: topPos, left: leftPos })
            $('#divImageDisplay').html("<img src='" + img.src + "' width='300px' height='300px' style='border: 3px solid #000;'/>");
        }

    </script>
    <script type="text/javascript">
        var varApplicationstr = 'some error occured !!';
        function GetApplicationDetail(APPLICATION_ID, leftPos, topPos) {
            $.ajax({
                type: "POST",
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                url: "RPTIHHLApplicationSummaryCSC.aspx/GetApplicationDetail",
                data: JSON.stringify({ 'APPLICATION_ID': APPLICATION_ID }),
                async: true,
                cache: false,
                success: function (msg) {
                    var returnmsg = msg.d;
                    $('#divImageDisplay').offset({ top: topPos, left: leftPos })
                    document.getElementById("ContentPlaceHolder1_hdnDetail").value = returnmsg;
                    $('#divImageDisplay').html("<div style='width:500px; background-color:white; border: 3px solid #000;'>" + document.getElementById("ContentPlaceHolder1_hdnDetail").value + "</div>");
                },
                beforeSend: function () {
                },
                complete: function () {
                },
                error: function () {
                    return "";
                }
            });
        }

        function getTopDetail(e) {
            var offset = e.offsetTop;
            if (e.offsetParent != null) offset += getTopDetail(e.offsetParent);
            return offset;
        }
        function getLeftDetail(e) {
            var offset = e.offsetLeft;
            if (e.offsetParent != null) offset += getLeftDetail(e.offsetParent);
            return offset;
        }
        function hideDetaileDisplay() {
            $('#divImageDisplay').html("");
        }

        function showDetailDisplay(img) {
            var grid = $('#ContentPlaceHolder1_gvApplicationList');
            var App = $('#' + img.id).text();
            var leftPos = getLeftDetail(img) + 20;
            var topPos = getTopDetail(img) - 150;
            GetApplicationDetail(App, leftPos, topPos);
        }

    </script>

