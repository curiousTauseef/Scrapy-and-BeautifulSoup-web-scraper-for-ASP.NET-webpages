         function ClearPassword() {
             var Pass = document.getElementById('ContentPlaceHolder1_txtCurrentPass');
             var NewPass = document.getElementById('ContentPlaceHolder1_txtNewPass');
             var ConfirmPass = document.getElementById('ContentPlaceHolder1_txtNewPassConfirm');
             if (Pass != null)
                 document.getElementById('ContentPlaceHolder1_txtCurrentPass').value = "";
             if (NewPass != null)
                 document.getElementById('ContentPlaceHolder1_txtNewPass').value = "";
             if (ConfirmPass != null)
                 document.getElementById('ContentPlaceHolder1_txtNewPassConfirm').value = "";
         }
    </script>
    
